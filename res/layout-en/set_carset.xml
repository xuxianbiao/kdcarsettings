<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:widget="http://schemas.android.com/apk/res/com.kandi.settings.widgets"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp" >
    <com.kandi.settings.widgets.SettingLeftTabsGroupLayout
        android:id="@+id/left_nav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    
    <FrameLayout 
        android:id="@+id/carset_container"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_toRightOf="@id/left_nav"
        android:layout_marginLeft="-2dp"
        android:background="@drawable/test_setting_tabs_bg"/>
        
</RelativeLayout>
