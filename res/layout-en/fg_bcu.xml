<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/cdjzgcddltxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/fg_bcu_battery_check"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/dianchizhanlianjiancezhouqi"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@+id/cdjzgcddltxt"
                android:background="@drawable/set_edittext_bg"
                android:hint=""
                android:inputType="number"
                android:textColor="@color/white" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/cdjzgcddltxt1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/fg_bcu_battery_circular"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/dianliuhuanxuanxing"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="68dp"
                android:layout_toRightOf="@+id/cdjzgcddltxt1"
                android:background="@drawable/set_edittext_bg"
                android:inputType="number"
                android:hint=""
                android:textColor="@color/white" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/cdjzgcddltxt2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/fg_bcu_battery_smallest"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/zuixiaodianchishumu"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="52dp"
                android:layout_toRightOf="@+id/cdjzgcddltxt2"
                android:background="@drawable/set_edittext_bg"
                android:hint=""
                android:inputType="number"
                android:textColor="@color/white" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/cdjzgcddltxt3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/fg_bcu_alarm_value"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/insulationwval"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:layout_marginLeft="68dp"
                android:layout_toRightOf="@+id/cdjzgcddltxt3"
                android:background="@drawable/set_edittext_bg"
                android:hint=""
                android:textColor="@color/white" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/cdjzgcddltxt4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/fg_bcu_bad_value"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/insulationeval"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="68dp"
                android:layout_toRightOf="@+id/cdjzgcddltxt4"
                android:background="@drawable/set_edittext_bg"
                android:hint=""
                android:inputType="number"
                android:textColor="@color/white" />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/fg_bcu_set"
            android:textColor="@color/darkgrey"
            android:textSize="16sp" />

        <RelativeLayout
            android:id="@+id/drivelock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" >

            <CheckBox
                android:id="@+id/radioButton1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/check_style"
                android:checked="true"
                android:text="@string/fg_bcu_set_all"
                android:textColor="@color/darkgrey" />

            <CheckBox
                android:id="@+id/radioButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/radioButton1"
                android:button="@drawable/check_style"
                android:checked="true"
                android:text="@string/fg_bcu_set_biggest"
                android:textColor="@color/darkgrey" />

            <CheckBox
                android:id="@+id/radioButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/radioButton1"
                android:layout_marginTop="10dp"
                android:button="@drawable/check_style"
                android:checked="true"
                android:text="@string/fg_bcu_set_kangdi"
                android:textColor="@color/darkgrey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/drivelock1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/drivemodetxtf1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/fg_bcu_RTC"
                android:textColor="@color/darkgrey"
                android:textSize="16sp" />

            <ToggleButton
                android:id="@+id/teamBuyBaoDiTog1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/drivemodetxtf1"
                android:layout_marginTop="10dp"
                android:background="@drawable/toggle_selector"
                android:textColor="@color/white"
                android:textOff=""
                android:textOn="" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/drivelock2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/drivemodetxtf2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/fg_bcu_RTC_CAN"
                android:textColor="@color/darkgrey"
                android:textSize="16sp" />

            <ToggleButton
                android:id="@+id/teamBuyBaoDiTog2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/drivemodetxtf2"
                android:layout_marginTop="10dp"
                android:background="@drawable/toggle_selector"
                android:textColor="@color/white"
                android:textOff=""
                android:textOn="" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/drivelock3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/drivemodetxtf3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/fg_bcu_battery_auto"
                android:textColor="@color/darkgrey"
                android:textSize="16sp" />

            <ToggleButton
                android:id="@+id/teamBuyBaoDiTog3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/drivemodetxtf3"
                android:layout_marginTop="10dp"
                android:background="@drawable/toggle_selector"
                android:textColor="@color/white"
                android:textOff=""
                android:textOn="" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/drivelock4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/drivemodetxtf4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/fg_bcu_balance"
                android:textColor="@color/darkgrey"
                android:textSize="16sp" />

            <ToggleButton
                android:id="@+id/teamBuyBaoDiTog4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/drivemodetxtf4"
                android:layout_marginTop="10dp"
                android:background="@drawable/toggle_selector"
                android:textColor="@color/white"
                android:textOff=""
                android:textOn="" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/drivelock5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/drivemodetxtf5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/fg_bcu_battery_sign"
                android:textColor="@color/darkgrey"
                android:textSize="16sp" />

            <ToggleButton
                android:id="@+id/teamBuyBaoDiTog5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/drivemodetxtf5"
                android:layout_marginTop="10dp"
                android:background="@drawable/toggle_selector"
                android:textColor="@color/white"
                android:textOff=""
                android:textOn="" />
        </RelativeLayout>

        <Button
            android:id="@+id/controlnormalbtn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/set_shoots_tap_btn_bg"
            android:text="@string/fg_bcu_save"
            android:textColor="@color/white"
            android:textSize="18sp" />
    </LinearLayout>

</ScrollView>